				<span class="sign" data-toggle="collapse" ><i class="icon-chevron-down"></i></span>
			</div>
			<div class="panel-collapse collapse in" id="print_settings_panel">
				<div class="form-group">
					<label class="sign" for="default_material_print_temperature">{{ _('Default Print Temperature') }}</label>
					<input id="default_material_print_temperature" class="inpt form-control" type="number"/>
					<span class="unit">ÂºC</span>
				</div>
				<div class="form-group">
					<label class="sign" for="speed_infill">{{ _('Infill Speed') }}</label>
					<input id="speed_infill" class="inpt form-control" type="number"/>
					<span class="unit">mm/s</span>
				</div>
				<div class="form-group">
					<label class="sign" for="speed_wall_0">{{ _('Outer Wall Speed') }}</label>
					<input id="speed_wall_0" class="inpt form-control" type="number"/>
					<span class="unit">mm/s</span>
				</div>
				<div class="form-group">
					<label class="sign" for="speed_wall_x">{{ _('Inner Wall Speed') }}</label>
					<input id="speed_wall_x" class="inpt form-control" type="number"/>
					<span class="unit">mm/s</span>
				</div>
			</div>
		</form>
	</div>
	<div class="modal-footer">
		<button class="btn" data-dismiss="modal" aria-hidden="true">{{ _('Abort') }}</button>
		<button class="btn btn-primary green" id="settings-cura2-new-edit"
				data-bind="click: function() { $root.saveMaterial($data); }">{{ _('Confirm') }}
		</button>
	</div>
</div>


