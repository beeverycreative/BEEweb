<div id="settings_plugin_curaX_change_name" class="modal hide fade">
    <div class="modal-header">
        <a href="#" class="close" data-dismiss="modal" aria-hidden="true">&times;</a>
        <a class="" href="#">
          <span class="signa" id ="profileQualityHeader"></span>
        </a>
    </div>
    <div class="modal-body parent">
        <form class="form-horizontal">
             <div class="panel-group" id="edit_panel">
                 <div class="panel_collapse panel-default">
                     <a class="" href="#">
                         <span class ="sign">NEW NAME:</span>
                         <input id = "_new_quality_name" class="inpt"  type="text" >
                     </a>
                 </div>
             </div>
        </form>
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary green" data-dismiss="modal" aria-hidden="true">{{ _('Abort') }}</button>
        <button class="btn btn-primary green" id="settings-cura2-new-edit" data-bind="click: function() { $root.changeQualityProfileName(); }">{{ _('Confirm') }}</button>
    </div>
</div>



<div id="settings_plugin_curaX_edit_profile" class="modal hide fade">
    <div class="modal-header">
        <a href="#" class="close" data-dismiss="modal" aria-hidden="true">&times;</a>
        <a class="" href="#">
            <span class="signa">EDIT PROFILE:</span>
            <span class="signa" id = "profileDisplay"></span>
        </a>
    </div>
    <div class="modal-body parent">
        <form class="form-horizontal">
            <div class="panel_collapse panel-default">
                     <div class="" href="#">
                         <span class="lbl">Profile:</span>
                         <select class="form-control" id="quality_droplist"></select>
                         <span class="sign" data-bind="click: function() {$root.removeQualityInProfile();}"><i class="icon-trash"></i></span>
                         <span class="sign" data-bind="click: function() {$root.NewQuality();}"><i class="icon-plus-sign-alt"></i></span>
                         <span class="sign" data-bind="click: function() {$root.NewQualityCopy();}"><i class="icon-copy"></i></span>
                         <span class="sign" data-bind="click: function() {$root.NewQualityName();}"><i class="icon-edit"></i></span>
                     </div>
            </div>
             <div class="panel-group" id="edit_content">

             </div>
        </form>
    </div>
    <div class="modal-footer">
        <button class="btn" data-dismiss="modal" aria-hidden="true">{{ _('Abort') }}</button>
        <button class="btn btn-primary green" id="settings-cura2-new-edit" data-bind="click: function() { $root.corfirmProfileEdition(); }">{{ _('Confirm') }}</button>
    </div>
</div>
