<div id = "settings_plugin_curaX_brands_interface">
     <a class="" href="#">
         <span class="lbl">BRANDS</span>
         <span data-toggle="collapse" href="#brands_tab" class="sign"><i class="icon-plus-sign-alt"></i></span>
     </a>
    <div id="brands_tab">
       <li><a class="" href="#">
            <input type="radio" name="brand" value="BEEVERYCREATIVE"  checked="checked">
            <span class="lbl">BEEVERYCREATIVE</span>
        </a></li>
        <li><a class="" href="#">
            <input type="radio" name="brand" value="BQ"  checked="checked">
            <span class="lbl">BQ</span>
        </a></li>
    </div>
</div>

<div id = "settings_plugin_curaX_material_interface">
    <a class="" href="#">
        <span class="lbl">MATERIALS</span>
        <span data-toggle="collapse"  href="#material_tab" class="sign"><i class=" icon-plus-sign-alt"></i></span>
    </a>
    <div id="material_tab">
        <div data-bind="foreach: materials.paginatedItems">
            <li class="item-9 deeper parent">
            <a class="" href="#">
                <span data-toggle="collapse" data-bind="attr: {'data-target': '#row' + $index()}" class="sign"><i class="icon-plus-sign-alt"></i></span>
                <span class= "lbl" data-bind = "text: description"></span>
            </a>
            <ul data-bind="attr: {id: 'row' + $index()}" class="collapse">
                <ul data-bind ="foreach: $parent.profiles.paginatedItems">
                    <ul  data-bind="if: $data.description() == $parent.description">
                        <li><a class="" href="#">
                                <span class="sign"><i class="icon-play"></i></span>
                                <span data-bind = "text:key"></span>
                                <span class="sign" data-bind="enable: !isdefault(), css: {disabled: isdefault()}, click: function() { if (!$data.isdefault()) { $root.editProfile($data); } }">
                                    <i class="icon-copy"></i></span>
                                <span class="sign" data-bind="enable: !isdefault(), css: {disabled: isdefault()}, click: function() { if (!$data.isdefault()) { $root.editProfile($data); } }">
                                    <i class="icon-trash"></i></span>
                                <span class="sign" data-bind="enable: !isdefault(), css: {disabled: isdefault()}, click: function() { if (!$data.isdefault()) { $root.editProfile($data); } }">
                                    <i class="icon-edit"></i></span>

                         </a></li>
                    </ul>
                </ul>
            </ul>
            </li>
        </div>
    </div>
</div>
